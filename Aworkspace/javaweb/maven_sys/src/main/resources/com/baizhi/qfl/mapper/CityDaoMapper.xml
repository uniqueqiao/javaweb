<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.baizhi.qfl.dao.CityDao">
   <insert id="insert" parameterType="City">
      insert into t_city(name) values (#{name})
   </insert>
   <select id="selectAll" resultType="City">
      select * from t_city
   </select>
   <select id="selectByName" resultType="City">
      select * from t_city where name=#{name}
   </select>
   <delete id="delete">
      delete from t_city where id=#{id}
   </delete>
</mapper>
